<?xml version="1.0" encoding="UTF-8"?>
<deploydescriptor>
	<global>
		<id>treeview</id>
		<title>Semantic Treeview extension</title>
		<version>1.4.5</version>
        <license>GPL-v2</license>
		<vendor>ontoprise GmbH</vendor>
		<maintainer>ontoprise GmbH</maintainer>
		<instdir>extensions/Treeview</instdir>
		<description>
			The Semantic Treeview extension automatically displays a hierarchical view of selected SMW+ elements such as categories, articles or semantically annotated data.
		</description>
		<helpurl>http://smwforum.ontoprise.com/smwforum/index.php/Help:TreeView_Extension</helpurl>
		<dependencies>
			<dependency from="1.6.0">smwhalo</dependency>
            <dependency from="1.5.6">smwplus</dependency>
		</dependencies>
		<notice/>
	</global>

	<codefiles>
		<file loc="deploy.xml" />
	</codefiles>

	<wikidumps>
		<file loc="treeview_sandbox.xml" />
	</wikidumps>

	<resources>
		<!-- empty -->
	</resources>

	<configs>
		<new>
			<require name="stv"
				file="extensions/Treeview/Treeview.php" />
		</new>
	</configs>
</deploydescriptor>